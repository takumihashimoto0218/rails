<head>
  <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
</head>
<h1>パック詳細画面</h1>
<h4 class="display-4 m-4">
<%= @pack.title %>
</h4>
<div class="group d-flex m-4">
  <div class="flex-row m-5">
    <% @topics.each do |topic| %>
      企画タイトル<h4><%= topic["title"] %></h4>
      <%= topic["id"] %>
    <% end %>
  </div>
  <%= link_to "編集", edit_pack_path(@pack) %>
  <button id="create-board-button" data-pack-id="<%= @pack.id %>">パックからボードを作成</button>
  <%= javascript_include_tag 'create-board' %>

</div>